using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ZZZ
{
    internal class Program
    {

        static void Main(string[] args)
        {

            int[] list1 = new int[3] { 5, 7, 9 };
            int[] list2 = new int[3] { 5, 9, 11 };
            int[] list3 = new int[6];
            for (int i = 0; i < list1.Length; i++)
            {
                if (list1[i] <= list2[i])
                {
                    Console.WriteLine("Left smaller");
                    Console.WriteLine(list1[i] + " " + list2[i]);
                }
                else
                {
                    Console.WriteLine("Right smaller");
                    Console.WriteLine(list2[i] + " " + list1[i]);
                }
            }
            Console.ReadLine();
        }
    }
}

